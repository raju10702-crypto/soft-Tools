    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        * { 
            margin: 0; 
            padding: 0; 
            box-sizing: border-box; 
        }
        
        :root {
            --primary-gradient: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            --secondary-gradient: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
            --accent-gradient: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);
            --success-gradient: linear-gradient(135deg, #43e97b 0%, #38f9d7 100%);
            --dark-bg: #0f172a;
            --darker-bg: #0a0f1c;
            --card-bg: #1e293b;
            --light-text: #f8fafc;
            --light-gray: #cbd5e1;
            --medium-gray: #64748b;
            --border-color: #334155;
            --shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
            --shadow-light: 0 10px 20px rgba(0, 0, 0, 0.2);
            --transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
            --border-radius: 20px;
            --border-radius-sm: 12px;
            --border-radius-lg: 30px;
        }
        
        body { 
            font-family: 'Inter', sans-serif; 
            line-height: 1.6; 
            color: var(--light-text); 
            background: var(--dark-bg); 
            min-height: 100vh;
            background-image: 
                radial-gradient(circle at 10% 20%, rgba(76, 29, 149, 0.1) 0%, transparent 20%),
                radial-gradient(circle at 90% 80%, rgba(101, 126, 234, 0.1) 0%, transparent 20%);
            padding-bottom: 60px;
        }
        
        .container { 
            width: 100%; 
            max-width: 1400px; 
            margin: 0 auto; 
            padding: 0 25px; 
        }
        
        /* Header Styling */
        header {
            background: rgba(15, 23, 42, 0.9);
            backdrop-filter: blur(10px);
            padding: 20px 0;
            position: sticky;
            top: 0;
            z-index: 1000;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
        }
        
        .header-content {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        
        .logo {
            display: flex;
            align-items: center;
            gap: 15px;
            font-family: 'Poppins', sans-serif;
            font-weight: 700;
            font-size: 1.8rem;
            background: var(--primary-gradient);
            -webkit-background-clip: text;
            background-clip: text;
            color: transparent;
            text-decoration: none;
            transition: var(--transition);
        }
        
        .logo:hover {
            transform: translateY(-2px);
        }
        
        .logo-icon {
            font-size: 2.2rem;
            display: flex;
            align-items: center;
            justify-content: center;
            width: 50px;
            height: 50px;
            background: var(--primary-gradient);
            border-radius: 15px;
            color: white;
            box-shadow: 0 10px 20px rgba(102, 126, 234, 0.3);
        }
        
        .nav-links {
            display: flex;
            gap: 35px;
        }
        
        .nav-link {
            color: var(--light-gray);
            text-decoration: none;
            font-weight: 500;
            transition: var(--transition);
            position: relative;
            padding: 5px 0;
        }
        
        .nav-link:hover {
            color: white;
        }
        
        .nav-link::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 0;
            height: 2px;
            background: var(--primary-gradient);
            transition: width 0.3s ease;
        }
        
        .nav-link:hover::after {
            width: 100%;
        }
        
        .mobile-menu-btn {
            display: none;
            background: none;
            border: none;
            font-size: 1.8rem;
            cursor: pointer;
            color: var(--light-gray);
            background: rgba(255, 255, 255, 0.05);
            width: 50px;
            height: 50px;
            border-radius: 12px;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        
        /* Hero Section */
        .hero-section {
            text-align: center;
            padding: 60px 0 40px;
            max-width: 900px;
            margin: 0 auto;
        }
        
        .hero-title {
            font-family: 'Poppins', sans-serif;
            font-size: 3.5rem;
            font-weight: 700;
            background: linear-gradient(90deg, #667eea, #f093fb, #4facfe);
            -webkit-background-clip: text;
            background-clip: text;
            color: transparent;
            margin-bottom: 20px;
            line-height: 1.2;
        }
        
        .hero-subtitle {
            font-size: 1.2rem;
            color: var(--light-gray);
            max-width: 700px;
            margin: 0 auto 40px;
            line-height: 1.8;
        }
        
        /* Main Content Layout */
        .main-content {
            display: grid;
            grid-template-columns: 1fr 350px;
            gap: 40px;
            margin-top: 20px;
        }
        
        @media (max-width: 1200px) {
            .main-content {
                grid-template-columns: 1fr;
            }
        }
        
        /* Tool Section */
        .tool-section {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 40px;
            box-shadow: var(--shadow);
            border: 1px solid var(--border-color);
            transition: var(--transition);
        }
        
        .tool-section:hover {
            box-shadow: 0 25px 50px rgba(0, 0, 0, 0.4);
        }
        
        .section-title {
            font-family: 'Poppins', sans-serif;
            font-size: 2.2rem;
            margin-bottom: 15px;
            color: var(--light-text);
            display: flex;
            align-items: center;
            gap: 15px;
        }
        
        .section-title i {
            background: var(--primary-gradient);
            width: 50px;
            height: 50px;
            border-radius: 12px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
        }
        
        .section-subtitle {
            color: var(--light-gray);
            margin-bottom: 40px;
            font-size: 1.1rem;
            line-height: 1.8;
        }
        
        /* Upload Area */
        .upload-area {
            border: 3px dashed var(--border-color);
            border-radius: var(--border-radius);
            padding: 70px 30px;
            text-align: center;
            margin-bottom: 40px;
            transition: var(--transition);
            cursor: pointer;
            position: relative;
            background: rgba(255, 255, 255, 0.02);
            overflow: hidden;
        }
        
        .upload-area::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(45deg, transparent 30%, rgba(102, 126, 234, 0.05) 50%, transparent 70%);
            background-size: 200% 200%;
            animation: shimmer 3s infinite linear;
        }
        
        @keyframes shimmer {
            0% { background-position: -200% -200%; }
            100% { background-position: 200% 200%; }
        }
        
        .upload-area:hover, .upload-area.dragover {
            border-color: #667eea;
            background: rgba(102, 126, 234, 0.05);
            transform: translateY(-5px);
        }
        
        .upload-icon {
            font-size: 4.5rem;
            margin-bottom: 25px;
            color: #667eea;
            display: inline-block;
            background: rgba(102, 126, 234, 0.1);
            width: 120px;
            height: 120px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 0 auto 25px;
            box-shadow: 0 15px 30px rgba(102, 126, 234, 0.2);
        }
        
        .upload-text {
            font-size: 1.4rem;
            margin-bottom: 15px;
            font-weight: 600;
            color: var(--light-text);
        }
        
        .upload-subtext {
            color: var(--light-gray);
            margin-bottom: 30px;
            font-size: 1.1rem;
        }
        
        .upload-btn {
            background: var(--primary-gradient);
            color: white;
            border: none;
            padding: 18px 40px;
            border-radius: var(--border-radius-sm);
            font-weight: 600;
            cursor: pointer;
            transition: var(--transition);
            display: inline-block;
            font-size: 1.1rem;
            box-shadow: 0 10px 20px rgba(102, 126, 234, 0.3);
            position: relative;
            overflow: hidden;
            z-index: 1;
        }
        
        .upload-btn::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: var(--secondary-gradient);
            z-index: -1;
            transition: opacity 0.3s ease;
            opacity: 0;
        }
        
        .upload-btn:hover::before {
            opacity: 1;
        }
        
        .upload-btn:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(102, 126, 234, 0.4);
        }
        
        .file-input { 
            display: none; 
        }
        
        .file-info {
            margin-top: 25px;
            padding: 20px;
            background: rgba(255, 255, 255, 0.03);
            border-radius: var(--border-radius-sm);
            border-left: 4px solid #667eea;
            display: none;
            animation: fadeIn 0.5s ease;
        }
        
        @keyframes fadeIn {
            from { opacity: 0; transform: translateY(10px); }
            to { opacity: 1; transform: translateY(0); }
        }
        
        .file-name {
            font-weight: 600;
            color: var(--light-text);
            display: block;
            margin-bottom: 5px;
            font-size: 1.1rem;
        }
        
        .file-size {
            color: var(--light-gray);
            font-size: 0.95rem;
        }
        
        /* Controls Section */
        .controls-section {
            margin-bottom: 40px;
        }
        
        .control-group {
            margin-bottom: 35px;
            background: rgba(255, 255, 255, 0.02);
            padding: 25px;
            border-radius: var(--border-radius-sm);
            border: 1px solid rgba(255, 255, 255, 0.05);
        }
        
        .control-label {
            display: block;
            font-weight: 600;
            margin-bottom: 20px;
            font-size: 1.2rem;
            color: var(--light-text);
            display: flex;
            align-items: center;
            gap: 10px;
        }
        
        .control-label i {
            color: #667eea;
            font-size: 1.3rem;
        }
        
        .compression-levels {
            display: flex;
            gap: 20px;
            flex-wrap: wrap;
        }
        
        .compression-level {
            flex: 1;
            min-width: 180px;
        }
        
        .level-btn {
            width: 100%;
            padding: 25px 20px;
            border: 2px solid var(--border-color);
            border-radius: var(--border-radius-sm);
            background: rgba(30, 41, 59, 0.7);
            cursor: pointer;
            transition: var(--transition);
            text-align: center;
        }
        
        .level-btn:hover {
            border-color: #667eea;
            background: rgba(102, 126, 234, 0.1);
            transform: translateY(-5px);
        }
        
        .level-btn.active {
            border-color: #667eea;
            background: rgba(102, 126, 234, 0.15);
            box-shadow: 0 10px 20px rgba(102, 126, 234, 0.2);
        }
        
        .level-name {
            font-weight: 700;
            margin-bottom: 8px;
            font-size: 1.2rem;
            color: var(--light-text);
        }
        
        .level-desc {
            font-size: 0.9rem;
            color: var(--light-gray);
            line-height: 1.5;
        }
        
        /* Quality Slider */
        .quality-slider-container {
            padding: 0 15px;
        }
        
        .slider-header {
            display: flex;
            justify-content: space-between;
            margin-bottom: 15px;
        }
        
        .quality-value {
            font-weight: 700;
            color: #667eea;
            font-size: 1.5rem;
        }
        
        .slider {
            width: 100%;
            height: 12px;
            border-radius: 10px;
            background: rgba(255, 255, 255, 0.05);
            outline: none;
            -webkit-appearance: none;
        }
        
        .slider::-webkit-slider-thumb {
            -webkit-appearance: none;
            width: 28px;
            height: 28px;
            border-radius: 50%;
            background: var(--primary-gradient);
            cursor: pointer;
            border: 4px solid var(--card-bg);
            box-shadow: 0 5px 15px rgba(102, 126, 234, 0.5);
        }
        
        /* Format Selection */
        .format-options {
            display: flex;
            gap: 20px;
            flex-wrap: wrap;
        }
        
        .format-option {
            padding: 18px 30px;
            border: 2px solid var(--border-color);
            border-radius: var(--border-radius-sm);
            background: rgba(30, 41, 59, 0.7);
            cursor: pointer;
            transition: var(--transition);
            font-weight: 600;
            color: var(--light-gray);
            flex: 1;
            min-width: 140px;
            text-align: center;
        }
        
        .format-option:hover {
            border-color: #667eea;
            background: rgba(102, 126, 234, 0.1);
            color: white;
            transform: translateY(-3px);
        }
        
        .format-option.active {
            border-color: #667eea;
            background: rgba(102, 126, 234, 0.15);
            color: white;
            box-shadow: 0 8px 15px rgba(102, 126, 234, 0.2);
        }
        
        /* Action Buttons */
        .action-buttons {
            display: flex;
            gap: 20px;
            margin-top: 40px;
        }
        
        .compress-btn, .reset-btn {
            flex: 1;
            padding: 22px;
            border-radius: var(--border-radius-sm);
            font-weight: 700;
            font-size: 1.1rem;
            cursor: pointer;
            transition: var(--transition);
            border: none;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 12px;
            position: relative;
            overflow: hidden;
            z-index: 1;
        }
        
        .compress-btn {
            background: var(--success-gradient);
            color: white;
            box-shadow: 0 10px 20px rgba(67, 233, 123, 0.3);
        }
        
        .compress-btn::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(135deg, #38f9d7 0%, #43e97b 100%);
            z-index: -1;
            transition: opacity 0.3s ease;
            opacity: 0;
        }
        
        .compress-btn:hover::before {
            opacity: 1;
        }
        
        .compress-btn:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(67, 233, 123, 0.4);
        }
        
        .reset-btn {
            background: rgba(255, 255, 255, 0.05);
            color: var(--light-gray);
            border: 2px solid var(--border-color);
        }
        
        .reset-btn:hover {
            background: rgba(255, 255, 255, 0.1);
            color: white;
            border-color: var(--light-gray);
            transform: translateY(-5px);
        }
        
        /* Results Section */
        .results-section {
            display: none;
            margin-top: 40px;
            padding-top: 40px;
            border-top: 1px solid var(--border-color);
            animation: fadeIn 0.8s ease;
        }
        
        .results-title {
            font-family: 'Poppins', sans-serif;
            font-size: 2rem;
            margin-bottom: 30px;
            color: var(--light-text);
            display: flex;
            align-items: center;
            gap: 15px;
        }
        
        .results-title i {
            background: var(--accent-gradient);
            width: 50px;
            height: 50px;
            border-radius: 12px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
        }
        
        .results-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 30px;
        }
        
        @media (max-width: 992px) {
            .results-container {
                grid-template-columns: 1fr;
            }
        }
        
        .image-comparison {
            display: flex;
            flex-direction: column;
            gap: 25px;
        }
        
        .image-box {
            border-radius: var(--border-radius);
            overflow: hidden;
            box-shadow: var(--shadow-light);
            background: rgba(255, 255, 255, 0.02);
            padding: 25px;
            border: 1px solid var(--border-color);
            transition: var(--transition);
        }
        
        .image-box:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(0, 0, 0, 0.3);
        }
        
        .image-label {
            display: flex;
            justify-content: space-between;
            margin-bottom: 20px;
            font-weight: 600;
            font-size: 1.1rem;
            color: var(--light-text);
        }
        
        .image-placeholder {
            width: 100%;
            height: 250px;
            display: flex;
            align-items: center;
            justify-content: center;
            background: rgba(255, 255, 255, 0.02);
            border-radius: var(--border-radius-sm);
            color: var(--light-gray);
            border: 2px dashed var(--border-color);
        }
        
        .image-preview {
            width: 100%;
            height: auto;
            border-radius: var(--border-radius-sm);
            display: block;
            max-height: 300px;
            object-fit: contain;
        }
        
        .stats-box {
            background: rgba(255, 255, 255, 0.02);
            border-radius: var(--border-radius);
            padding: 30px;
            box-shadow: var(--shadow-light);
            border: 1px solid var(--border-color);
        }
        
        .stats-title {
            font-family: 'Poppins', sans-serif;
            font-size: 1.8rem;
            margin-bottom: 25px;
            color: var(--light-text);
            display: flex;
            align-items: center;
            gap: 15px;
        }
        
        .stats-title i {
            background: var(--secondary-gradient);
            width: 50px;
            height: 50px;
            border-radius: 12px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
        }
        
        .stat-item {
            display: flex;
            justify-content: space-between;
            padding: 18px 0;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
        }
        
        .stat-item:last-child {
            border-bottom: none;
        }
        
        .stat-name {
            color: var(--light-gray);
            display: flex;
            align-items: center;
            gap: 10px;
        }
        
        .stat-name i {
            color: #667eea;
            font-size: 1.1rem;
        }
        
        .stat-value {
            font-weight: 700;
            font-size: 1.2rem;
            color: var(--light-text);
        }
        
        .savings {
            color: #43e97b;
            font-weight: 700;
        }
        
        .download-btn {
            display: block;
            width: 100%;
            margin-top: 30px;
            padding: 22px;
            background: var(--accent-gradient);
            color: white;
            border: none;
            border-radius: var(--border-radius-sm);
            font-weight: 700;
            font-size: 1.2rem;
            cursor: pointer;
            transition: var(--transition);
            text-align: center;
            text-decoration: none;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 15px;
            box-shadow: 0 10px 20px rgba(79, 172, 254, 0.3);
        }
        
        .download-btn:hover {
            background: linear-gradient(135deg, #00f2fe 0%, #4facfe 100%);
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(79, 172, 254, 0.4);
        }
        
        /* Ads Section */
        .ads-section {
            display: flex;
            flex-direction: column;
            gap: 30px;
        }
        
        .ad-container {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 25px;
            box-shadow: var(--shadow);
            min-height: 300px;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            text-align: center;
            border: 1px solid var(--border-color);
            transition: var(--transition);
        }
        
        .ad-container:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(0, 0, 0, 0.4);
        }
        
        .ad-label {
            font-size: 0.9rem;
            color: var(--light-gray);
            margin-bottom: 15px;
            text-transform: uppercase;
            letter-spacing: 2px;
            background: rgba(255, 255, 255, 0.05);
            padding: 8px 20px;
            border-radius: 30px;
        }
        
        .ad-placeholder {
            width: 100%;
            height: 250px;
            background: rgba(255, 255, 255, 0.02);
            border-radius: var(--border-radius-sm);
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            color: var(--light-gray);
            margin-bottom: 20px;
            border: 2px dashed var(--border-color);
        }
        
        .ad-placeholder.large {
            height: 600px;
        }
        
        .ad-icon {
            font-size: 3.5rem;
            margin-bottom: 20px;
            color: var(--medium-gray);
        }
        
        .ad-instruction {
            font-size: 0.95rem;
            color: var(--light-gray);
            max-width: 280px;
            line-height: 1.6;
        }
        
        /* Features Section */
        .features-section {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 30px;
            margin-top: 60px;
        }
        
        .feature-card {
            background: var(--card-bg);
            border-radius: var(--border-radius);
            padding: 35px 30px;
            text-align: center;
            border: 1px solid var(--border-color);
            transition: var(--transition);
        }
        
        .feature-card:hover {
            transform: translateY(-10px);
            box-shadow: var(--shadow);
        }
        
        .feature-icon {
            font-size: 3rem;
            margin-bottom: 25px;
            display: inline-block;
            background: var(--primary-gradient);
            -webkit-background-clip: text;
            background-clip: text;
            color: transparent;
        }
        
        .feature-title {
            font-family: 'Poppins', sans-serif;
            font-size: 1.5rem;
            margin-bottom: 15px;
            color: var(--light-text);
        }
        
        .feature-desc {
