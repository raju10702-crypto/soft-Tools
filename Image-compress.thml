    <script>
        // DOM Elements
        const uploadArea = document.getElementById('uploadArea');
        const browseBtn = document.getElementById('browseBtn');
        const fileInput = document.getElementById('fileInput');
        const compressBtn = document.getElementById('compressBtn');
        const resetBtn = document.getElementById('resetBtn');
        const qualitySlider = document.getElementById('qualitySlider');
        const qualityValue = document.getElementById('qualityValue');
        const levelButtons = document.querySelectorAll('.level-btn');
        const formatButtons = document.querySelectorAll('.format-option');
        const resultsSection = document.getElementById('resultsSection');
        const loadingIndicator = document.getElementById('loadingIndicator');
        const fileInfo = document.getElementById('fileInfo');
        const fileName = document.getElementById('fileName');
        const fileSize = document.getElementById('fileSize');
        const mobileMenuBtn = document.getElementById('mobileMenuBtn');
        const navLinks = document.getElementById('navLinks');
        
        // Image preview elements
        const originalImagePlaceholder = document.getElementById('originalImagePlaceholder');
        const compressedImagePlaceholder = document.getElementById('compressedImagePlaceholder');
        const originalImagePreview = document.getElementById('originalImagePreview');
        const compressedImagePreview = document.getElementById('compressedImagePreview');
        
        // Stat elements
        const originalSizeElement = document.getElementById('originalSize');
        const compressedSizeElement = document.getElementById('compressedSize');
        const statOriginalSize = document.getElementById('statOriginalSize');
        const statCompressedSize = document.getElementById('statCompressedSize');
        const statReduction = document.getElementById('statReduction');
        const statLevel = document.getElementById('statLevel');
        const statQuality = document.getElementById('statQuality');
        const downloadBtn = document.getElementById('downloadBtn');
        
        // State variables
        let originalImage = null;
        let originalFileSize = 0;
        let compressedImageBlob = null;
        let currentCompressionLevel = 'low';
        let currentFormat = 'original';
        let currentQuality = 85;
        
        // Initialize the tool when DOM is loaded
        document.addEventListener('DOMContentLoaded', function() {
            console.log('Image Compression Tool initialized');
            setupEventListeners();
            
            // Set initial compression level
            document.querySelector('.level-btn[data-level="low"]').classList.add('active');
            document.querySelector('.format-option[data-format="original"]').classList.add('active');
            qualitySlider.value = 85;
            qualityValue.textContent = '85%';
        });
        
        function setupEventListeners() {
            // Browse button click event
            browseBtn.addEventListener('click', function(e) {
                e.preventDefault();
                e.stopPropagation();
                console.log('Browse button clicked');
                fileInput.click();
            });
            
            // File input change event
            fileInput.addEventListener('change', function(e) {
                console.log('File input changed');
                if (this.files && this.files[0]) {
                    const file = this.files[0];
                    
                    // Check file size (limit to 10MB)
                    if (file.size > 10 * 1024 * 1024) {
                        alert('File size exceeds 10MB limit. Please choose a smaller image.');
                        return;
                    }
                    
                    processFile(file);
                }
            });
            
            // Drag and drop events for upload area
            uploadArea.addEventListener('dragover', function(e) {
                e.preventDefault();
                e.stopPropagation();
                console.log('Drag over upload area');
                this.classList.add('dragover');
            });
            
            uploadArea.addEventListener('dragleave', function(e) {
                e.preventDefault();
                e.stopPropagation();
                console.log('Drag leave upload area');
                this.classList.remove('dragover');
            });
            
            uploadArea.addEventListener('drop', function(e) {
                e.preventDefault();
                e.stopPropagation();
                console.log('File dropped on upload area');
                this.classList.remove('dragover');
                
                if (e.dataTransfer.files.length) {
                    const file = e.dataTransfer.files[0];
                    
                    // Check file size (limit to 10MB)
                    if (file.size > 10 * 1024 * 1024) {
                        alert('File size exceeds 10MB limit. Please choose a smaller image.');
                        return;
                    }
                    
                    processFile(file);
                }
            });
            
            // Click on upload area to trigger file input
            uploadArea.addEventListener('click', function(e) {
                console.log('Upload area clicked');
                // Only trigger file input if not clicking the browse button
                if (e.target !== browseBtn && !browseBtn.contains(e.target) && e.target !== fileInput) {
                    fileInput.click();
                }
            });
            
            // Quality slider event
            qualitySlider.addEventListener('input', function() {
                currentQuality = parseInt(this.value);
                qualityValue.textContent = `${currentQuality}%`;
                console.log('Quality changed to:', currentQuality);
            });
            
            // Compression level buttons
            levelButtons.forEach(button => {
                button.addEventListener('click', function() {
                    levelButtons.forEach(btn => btn.classList.remove('active'));
                    this.classList.add('active');
                    currentCompressionLevel = this.getAttribute('data-level');
                    console.log('Compression level changed to:', currentCompressionLevel);
                    updateQualityBasedOnLevel();
                });
            });
            
            // Format buttons
            formatButtons.forEach(button => {
                button.addEventListener('click', function() {
                    formatButtons.forEach(btn => btn.classList.remove('active'));
                    this.classList.add('active');
                    currentFormat = this.getAttribute('data-format');
                    console.log('Output format changed to:', currentFormat);
                });
            });
            
            // Compress button
            compressBtn.addEventListener('click', function() {
                console.log('Compress button clicked');
                compressImage();
            });
            
            // Reset button
            resetBtn.addEventListener('click', function() {
                console.log('Reset button clicked');
                resetTool();
            });
            
            // Mobile menu toggle
            mobileMenuBtn.addEventListener('click', function() {
                navLinks.classList.toggle('active');
                mobileMenuBtn.innerHTML = navLinks.classList.contains('active') 
                    ? '<i class="fas fa-times"></i>' 
                    : '<i class="fas fa-bars"></i>';
            });
            
            // Close mobile menu when clicking outside
            document.addEventListener('click', function(e) {
                if (!navLinks.contains(e.target) && !mobileMenuBtn.contains(e.target)) {
                    navLinks.classList.remove('active');
                    mobileMenuBtn.innerHTML = '<i class="fas fa-bars"></i>';
                }
            });
            
            // Window resize event
            window.addEventListener('resize', function() {
                if (window.innerWidth > 768) {
                    navLinks.classList.remove('active');
                    mobileMenuBtn.innerHTML = '<i class="fas fa-bars"></i>';
                }
            });
        }
        
        function processFile(file) {
            console.log('Processing file:', file.name, file.type, file.size);
            
            // Check if file is an image
            if (!file.type.match('image.*')) {
                alert('Please select an image file (JPEG, PNG, WebP, GIF)');
                return;
            }
            
            // Update file info display
            fileName.textContent = file.name;
            fileSize.textContent = `(${formatFileSize(file.size)})`;
            fileInfo.style.display = 'block';
            
            // Update file size
            originalFileSize = file.size;
            originalSizeElement.textContent = formatFileSize(originalFileSize);
            statOriginalSize.textContent = formatFileSize(originalFileSize);
            
            // Load image for preview
            const reader = new FileReader();
            
            reader.onload = function(e) {
                console.log('File read successfully');
                originalImage = new Image();
                originalImage.onload = function() {
                    console.log('Image loaded successfully, dimensions:', originalImage.width, 'x', originalImage.height);
                    // Show original image preview
                    originalImagePlaceholder.style.display = 'none';
                    originalImagePreview.src = e.target.result;
                    originalImagePreview.style.display = 'block';
                    
                    // Show the results section
                    resultsSection.style.display = 'block';
                    
                    // Reset compressed image preview
                    compressedImagePreview.style.display = 'none';
                    compressedImagePlaceholder.style.display = 'flex';
                    compressedImagePlaceholder.textContent = 'Image will appear here after compression';
                    compressedSizeElement.textContent = '0 KB';
                    
                    // Enable compress button
                    compressBtn.disabled = false;
                    
                    // Smooth scroll to results
                    setTimeout(() => {
                        resultsSection.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
                    }, 300);
                };
                originalImage.onerror = function() {
                    console.error('Error loading image');
                    alert('Error loading image. Please try another file.');
                };
                originalImage.src = e.target.result;
            };
            
            reader.onerror = function() {
                console.error('Error reading file');
                alert('Error reading file. Please try again.');
            };
            
            reader.readAsDataURL(file);
        }
        
        function updateQualityBasedOnLevel() {
            let qualityValue;
            switch(currentCompressionLevel) {
                case 'low':
                    qualityValue = 85;
                    break;
                case 'medium':
                    qualityValue = 65;
                    break;
                case 'high':
                    qualityValue = 40;
                    break;
            }
            qualitySlider.value = qualityValue;
            currentQuality = qualityValue;
            qualityValue.textContent = `${qualityValue}%`;
        }
        
        function compressImage() {
            if (!originalImage) {
                alert('Please upload an image first');
                return;
            }
            
            console.log('Starting compression...');
            console.log('Compression level:', currentCompressionLevel);
            console.log('Quality:', currentQuality);
            console.log('Format:', currentFormat);
            
            // Show loading indicator
            loadingIndicator.style.display = 'block';
            compressBtn.disabled = true;
            
            // Use setTimeout to show loading animation
            setTimeout(() => {
                // Create canvas for image compression
                const canvas = document.createElement('canvas');
                const ctx = canvas.getContext('2d');
                
                // Set canvas dimensions to image dimensions
                canvas.width = originalImage.width;
                canvas.height = originalImage.height;
                
                // Draw image on canvas
                ctx.drawImage(originalImage, 0, 0);
                
                // Determine output format
                let format = currentFormat === 'original' 
                    ? getFileExtension(fileInput.files[0].name) 
                    : currentFormat;
                
                // Adjust quality based on compression level
                let quality = currentQuality / 100;
                
                // For PNG format, quality parameter doesn't work the same way
                if (format === 'png') {
                    // For PNG, we can't directly control compression with quality parameter
                    const dataURL = canvas.toDataURL(`image/${format}`);
                    fetch(dataURL)
                        .then(res => res.blob())
                        .then(blob => {
                            handleCompressedBlob(blob, format);
                        })
                        .catch(error => {
                            console.error('Compression error:', error);
                            alert('Compression failed. Please try again.');
                            loadingIndicator.style.display = 'none';
                            compressBtn.disabled = false;
                        });
                } else {
                    // For JPEG and WebP, use toBlob with quality parameter
                    canvas.toBlob((blob) => {
                        handleCompressedBlob(blob, format);
                    }, `image/${format === 'jpeg' ? 'jpeg' : format}`, quality);
                }
            }, 500); // Small delay to show loading animation
        }
        
        function handleCompressedBlob(blob, format) {
            console.log('Compression complete, blob size:', blob.size);
            
            // Hide loading indicator
            loadingIndicator.style.display = 'none';
            compressBtn.disabled = false;
            
            if (!blob) {
                alert('Compression failed. Please try again.');
                return;
            }
            
            compressedImageBlob = blob;
            
            // Update compressed image preview
            const compressedUrl = URL.createObjectURL(blob);
            compressedImagePreview.src = compressedUrl;
            compressedImagePreview.style.display = 'block';
            compressedImagePlaceholder.style.display = 'none';
            
            // Update file sizes
            const compressedFileSize = blob.size;
            compressedSizeElement.textContent = formatFileSize(compressedFileSize);
            statCompressedSize.textContent = formatFileSize(compressedFileSize);
            
            // Calculate and display reduction percentage
            const reduction = 100 - (compressedFileSize / originalFileSize * 100);
            statReduction.textContent = `${reduction.toFixed(1)}%`;
            
            // Update other stats
            statLevel.textContent = currentCompressionLevel.charAt(0).toUpperCase() + currentCompressionLevel.slice(1);
            statQuality.textContent = `${currentQuality}%`;
            
            // Update download button
            downloadBtn.href = compressedUrl;
            const extension = format === 'jpeg' ? 'jpg' : format;
            downloadBtn.download = `compressed-image.${extension}`;
            downloadBtn.textContent = `Download ${extension.toUpperCase()} (${formatFileSize(compressedFileSize)})`;
            
            console.log('Compression results:');
            console.log('Original size:', originalFileSize);
            console.log('Compressed size:', compressedFileSize);
            console.log('Reduction:', reduction.toFixed(1) + '%');
            
            // Scroll to download button
            setTimeout(() => {
                downloadBtn.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
            }, 300);
        }
        
        function resetTool() {
            console.log('Resetting tool...');
            
            // Reset file input
            fileInput.value = '';
            originalImage = null;
            originalFileSize = 0;
            compressedImageBlob = null;
            
            // Reset UI
            originalImagePlaceholder.style.display = 'flex';
            originalImagePlaceholder.innerHTML = '<i class="fas fa-image" style="font-size: 3rem; margin-bottom: 15px;"></i><div>No image uploaded</div>';
            originalImagePreview.style.display = 'none';
            originalImagePreview.src = '';
            originalSizeElement.textContent = '0 KB';
            
            compressedImagePlaceholder.style.display = 'flex';
            compressedImagePlaceholder.innerHTML = '<i class="fas fa-hourglass-half" style="font-size: 3rem; margin-bottom: 15px;"></i><div>Image will appear here after compression</div>';
            compressedImagePreview.style.display = 'none';
            compressedImagePreview.src = '';
            compressedSizeElement.textContent = '0 KB';
            
            resultsSection.style.display = 'none';
            fileInfo.style.display = 'none';
            
            // Reset compression settings
            levelButtons.forEach(btn => btn.classList.remove('active'));
            document.querySelector('.level-btn[data-level="low"]').classList.add('active');
            currentCompressionLevel = 'low';
            
            formatButtons.forEach(btn => btn.classList.remove('active'));
            document.querySelector('.format-option[data-format="original"]').classList.add('active');
            currentFormat = 'original';
            
            qualitySlider.value = 85;
            currentQuality = 85;
            qualityValue.textContent = '85%';
            
            // Revoke object URLs to free memory
            if (compressedImagePreview.src.startsWith('blob:')) {
                URL.revokeObjectURL(compressedImagePreview.src);
            }
            if (originalImagePreview.src.startsWith('blob:')) {
                URL.revokeObjectURL(originalImagePreview.src);
            }
            
            console.log('Tool reset complete');
        }
        
        function formatFileSize(bytes) {
            if (bytes === 0) return '0 Bytes';
            
            const k = 1024;
            const sizes = ['Bytes', 'KB', 'MB', 'GB'];
            const i = Math.floor(Math.log(bytes) / Math.log(k));
            
            return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
        }
        
        function getFileExtension(filename) {
            return filename.slice((filename.lastIndexOf(".") - 1 >>> 0) + 2).toLowerCase();
        }
        
        // AdSense integration instructions
        console.log('To integrate Google AdSense:');
        console.log('1. Replace the ad placeholders with your AdSense code');
        console.log('2. Ad locations: #ad-unit-top, #ad-unit-side1, #ad-unit-side2');
    </script>
</body>
</html>
